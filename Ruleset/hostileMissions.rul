ufoTrajectories:
  - id: __RETALIATION_ASSAULT_RUN
    groundTimer: 1000
    waypoints:
      - [5, 1, 100] #This should always be 5, 4, 100
      - [5, 0, 100] #only the 0 altitude matters

  - id: CRASH_CHECKOUT
    groundTimer: 3600
    waypoints:
      - [5, 3, 100] # Start at base
      - [0, 2, 60] # Move to region
      - [8, 1, 25] # Move to sub-region
      - [8, 0, 30] # Land in sub-region
      - [8, 1, 25] # Move in sub-region
      - [8, 0, 30] # Land in sub-region
      - [7, 1, 25] # Move to sub-region
      - [7, 0, 30] # Land in sub-region
      - [7, 1, 25] # Move in sub-region
      - [7, 0, 30] # Land in sub-region
      - [6, 1, 25] # Move to sub-region
      - [6, 0, 30] # Land in sub-region
      - [6, 1, 25] # Move in sub-region
      - [6, 0, 30] # Land in sub-region
      - [0, 3, 60] # Take off
      - [5, 0, 100] # Land at base

  - id: MINING_SCOUT
    groundTimer: 60
    waypoints:
      - [5, 3, 100] # Start at base
      - [0, 2, 60] # Move to region
      - [8, 1, 30] # Move to sub-region
      - [8, 1, 25] # Scout within region
      - [8, 0, 30] # Land once
      - [8, 1, 30] # Check out another site
      - [8, 1, 25]
      - [8, 0, 30]
      - [8, 1, 30] # Check out one more site
      - [8, 1, 25]
      - [8, 0, 30]
      - [0, 3, 60] # Take off again
      - [5, 0, 100] # Land at base

  - id: MINING_LANDING
    groundTimer: 3000
    waypoints:
      - [5, 3, 100] # Start at base
      - [0, 2, 60] # Move to region
      - [8, 1, 25] # Move to sub-region
      - [8, 0, 30] # Land once
      - [8, 1, 25] # Try another spot
      - [8, 0, 30]
      - [0, 3, 60] # Take off again
      - [5, 0, 100] # Land at base

  - id: MINING_LANDING_FAMINE
    groundTimer: 6000 # Needs to stay longer to get more materials, makes up for smaller waves
    waypoints:
      - [5, 3, 100] # Start at base
      - [0, 2, 60] # Move to region
      - [8, 1, 25] # Move to sub-region
      - [8, 0, 30] # Land once
      - [8, 1, 25] # Try another spot
      - [8, 0, 30]
      - [0, 3, 60] # Take off again
      - [5, 0, 100] # Land at base

  - id: MINING_ESCORT
    waypoints:
      - [5, 3, 100] # Start at base
      - [0, 2, 60] # Move to region
      - [8, 2, 45] # Just fly around the sub-region for a while
      - [8, 2, 45]
      - [8, 2, 45]
      - [8, 2, 45]
      - [8, 2, 45]
      - [8, 2, 45]
      - [8, 2, 45]
      - [8, 2, 45]
      - [0, 3, 60] # Get ready to head out
      - [5, 0, 100] # Land at base

