alienMissions:
  - type: STR_CRASH_INVESTIGATION # First thing aliens do is check your crash site out
    points: 0
    objective: 4
    retaliationOdds: 0
    missionWeights:
      0: 0
    spawnUfo: STR_LARGE_SCOUT
    raceWeights:
      0:
        STR_MINING_CREW: 100
    waves:
      - ufo: STR_MEDIUM_SCOUT
        count: 2
        trajectory: CRASH_CHECKOUT
        timer: 60 # Spawn first two ufos within an hour of each other
      - ufo: STR_LARGE_SCOUT
        count: 1
        trajectory: CRASH_CHECKOUT
        timer: 5760 # Give the first wave 2-6 days to finish patrol
      - ufo: STR_LARGE_SCOUT
        count: 1
        trajectory: CRASH_CHECKOUT
        timer: 60 # Flies along with the previous one

  - type: STR_MINING_ACTIVITY_FAMINE # Monthly activity, meant to be a source of player income and points
    points: 25
    raceWeights:
      0:
        STR_MINING_CREW: 100
    waves:
      - ufo: STR_MEDIUM_SCOUT
        count: 1
        trajectory: MINING_SCOUT
        timer: 0
      #- ufo: STR_MEDIUM_SCOUT
      #  count: 2
      #  trajectory: MINING_LANDING
      #  timer: 1440
      - ufo: STR_LARGE_SCOUT
        count: 1
        trajectory: MINING_LANDING
        timer: 1440

  - type: STR_MINING_ESCORT_FAMINE # Mostly for flavor to escort the mining ships, but can be shot down later for more points
    points: 25
    raceWeights:
      0:
        STR_MILITARY_CREW: 100
    waves:
      - ufo: STR_ABDUCTOR
        count: 1
        trajectory: MINING_ESCORT
        timer: 0
      - ufo: STR_ABDUCTOR
        count: 2 # 4
        trajectory: MINING_ESCORT
        timer: 1440

  - type: STR_MINING_ACTIVITY_PLAGUE
    points: 25
    raceWeights:
      0:
        STR_MINING_CREW: 100
    waves:
      - ufo: STR_MEDIUM_SCOUT
        count: 1
        trajectory: MINING_SCOUT
        timer: 0
      - ufo: STR_MEDIUM_SCOUT
        count: 2
        trajectory: MINING_LANDING
        timer: 1440
      - ufo: STR_LARGE_SCOUT
        count: 1
        trajectory: MINING_LANDING
        timer: 1440

  - type: STR_MINING_ESCORT_PLAGUE
    points: 25
    raceWeights:
      0:
        STR_MILITARY_CREW: 100
    waves:
      - ufo: STR_ABDUCTOR
        count: 1
        trajectory: MINING_ESCORT
        timer: 0
      - ufo: STR_ABDUCTOR
        count: 4
        trajectory: MINING_ESCORT
        timer: 1440

  - type: STR_MINING_ACTIVITY_WAR
    points: 25
